@layout("/common/_container.html"){
<meta name="viewport" content="initial-scale=1.0, user-scalable=no" />
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>Baidu Map </title>
<style type="text/css">
    html{height:100%}
    body{height:100%;margin:0px;padding:0px}
    #container{height:500px;width: 100%;}
</style>
<div class="ibox float-e-margins">
    <input type="hidden" id="page" value="ZBXM">
    <input type="hidden" id="pageType" value="view">
    <div id="info" data-flag="true" style="display: none;"></div>
    <div class="tabs-container">
        <ul class="nav nav-tabs">
            <li class="active">
                <a data-toggle="tab" href="#projectInfo" aria-expanded="true"> 项目信息</a>
            </li>
            <li class="">
                <a data-toggle="tab" onclick="ProjectMasterInfoDlg.anchorCheck()"  href="#joinedSubContractor" aria-expanded="false">参建单位</a>
            </li>
            <li class="">
                <a data-toggle="tab" href="#classList"  aria-expanded="false">班组列表</a>
            </li>
        </ul>
        <div class="tab-content">
            <div id="projectInfo" class="tab-pane active">
                <div class="panel-body">
                    <div class="ibox-content">
                        <div class="form-horizontal">

                            <input type="hidden" id="id" value="${item.id}">

                            <div class="row">
                                <div class="col-sm-6 b-r">
                                    <#input_sjf id="projectName" name="项目名称" underline="true" requiredFlag="true" value="${item.projectName}" disabled="disabled"/>
                                    <#input_sjf id="projectCode" name="项目编号" underline="true" visible="true" requiredFlag="true" value="${item.projectCode}" disabled="disabled"/>
                                    <#input_sjf id="generalContractorCode" name="承包单位编号" underline="true" disabled="disabled" value="${item.generalContractorCode}" disabled="disabled"/>
                                    <#select_sjf id="isMajorProject" name="重点项目" underline="true" requiredFlag="true" value="${item.isMajorProject}" disabled="disabled">
                                        <option value="0">否</option>
                                        <option value="1">是</option>
                                    </#select_sjf>
                                    <#input_sjf id="ownerName" name="建设单位名称" underline="true" requiredFlag="true" value="${item.ownerName}" disabled="disabled"/>

                                    <#select_sjf id="projectActivityType" name="项目活动类型" underline="true" requiredFlag="true" value="${item.projectActivityType}" disabled="disabled">
                                        @for(item in projectActivityType){
                                        <option value="${item.num}">${item.name}</option>
                                        @}
                                    </#select_sjf>
                                    <#input_sjf id="builderLicenceNum" name="施工许可证编号" underline="true" requiredFlag="true" value="${item.builderLicenceNum}" disabled="disabled"/>
                                    <#input_sjf id="buildingArea" name="建筑面积(平方米)" underline="true" value="${item.buildingArea}" disabled="disabled"/>
                                    <#input_sjf id="address" name="详细地址" underline="true"  value="${item.address}" disabled="disabled"/>
                                    <#select_sjf id="deviceType" name="考勤类型" underline="true" disabled="disabled">
                                        @for(i in deviceType){
                                        @if(i.num==item.deviceType){
                                        <option value="${i.num}" selected>${i.name}</option>
                                        @}else{
                                        <option value="${i.num}">${i.name}</option>
                                        @}
                                        @}
                                    </#select_sjf>
                                    <#input_sjf id="startDate" name="开工时间" underline="true" type="date" clickFun="laydate({istime: false, format: 'YYYY-MM-DD'})"  disabled="disabled" requiredFlag="true" value="${item.startDate}"/>
                                    <#input_sjf id="projectSource" name="项目来源" underline="true" value="${item.projectSource}" disabled="disabled"/>
                                </div>

                                <div class="col-sm-6">

                                    <!--<div class="row">
                                        <div class="col-lg-6">
                                            <div class="input-group" style="width: 300px;">
                                                <text style="color:red;">*</text>
                                                承包商名称
                                                <input type="text" class="form-control" disabled="disabled"  id="generalContractorName" autocomplete="off" value="${item.generalContractorName}">
                                                <div class="input-group-btn">
                                                    <button type="button" class="btn btn-white dropdown-toggle" data-toggle="">
                                                        <span>搜索</span>
                                                    </button>
                                                    <ul class="dropdown-menu dropdown-menu-right" role="menu"
                                                        style="padding-top: 0px; max-height: 375px; max-width: 800px; overflow: auto; width: auto; transition: 0.3s; left: -267px; right: auto; min-width: 300px;">

                                                    </ul>
                                                </div>
                                                &lt;!&ndash; /btn-group &ndash;&gt;
                                            </div>
                                        </div>
                                    </div>-->
                                    <#input_sjf id="generalContractorName" name="承包商名称" underline="true" requiredFlag="true" disabled="disabled" value="${item.generalContractorName}"/>
                                    <#input_sjf id="contractorOrgCode" name="组织机构代码" underline="true" requiredFlag="true" disabled="disabled" value="${item.contractorOrgCode}"/>
                                    <#select_sjf id="projectCategory" name="项目分类" underline="true"  requiredFlag="true" value="${item.projectCategory}" disabled="disabled">
                                        @for(item in projectCategory){
                                        <option value="${item.num}">${item.name}</option>
                                        @}
                                    </#select_sjf>
                                    <#input_sjf id="buildCorporationCode" name="社会信用代码" underline="true" requiredFlag="true" disabled="disabled" value="${item.buildCorporationCode}"/>
                                    <#input_sjf id="areaName" name="项目所在地" requiredFlag="true" value="${item.areaName}" disabled="disabled"/>
                                    <#input_sjf id="totalContractAmt" name="承包合同额(万元)" underline="true" requiredFlag="true" value="${item.totalContractAmt}" disabled="disabled"/>
                                    <#input_sjf id="completeDate" name="竣工时间" underline="true" type="date" clickFun="laydate({istime: false, format: 'YYYY-MM-DD'})"  disabled="disabled" requiredFlag="true" value="${item.completeDate}"/>
                                    <#select_sjf id="projectStatus" name="项目状态" underline="true" requiredFlag="true"  disabled="disabled">
                                        @for(i in projectStatus){
                                        @if(""+i.num==item.projectStatus){
                                        <option value="${i.num}" selected>${i.name}</option>
                                        @}else{
                                        <option value="${i.num}">${i.name}</option>
                                        @}
                                        @}
                                    </#select_sjf>
                                    <#input_sjf id="radius" name="考勤范围" underline="true" value="${item.radius}" disabled="disabled"/>
                                    <#input_sjf id="lng" name="经度" underline="true" value="${item.lng}" disabled="disabled"/>
                                    <#input_sjf id="lat" name="纬度" underline="true" value="${item.lat}" disabled="disabled"/>
                                </div>
                            </div>

                            <div id="container">

                            </div>

                        </div>

                    </div>
                </div>
            </div>
            <div id="joinedSubContractor" class="tab-pane">
                <div class="ibox-content">
                    <div class="row row-lg">
                        <div class="col-sm-12">
                            <div class="row">
                                <div class="col-lg-10">
                                    <div class="inline-search-s">
                                        <label class="label-search-s">查询</label>
                                        <input type="text" class="input-search-s" id="condition" placeholder="" />
                                    </div>

                                    <div class="inline-search-s">
                                        <label class="label-search-s">项目状态</label>
                                        <select id="queryProjectStatus" class="input-search-s" name="项目状态" underline="true">
                                            <option value="">请选择</option>
                                            @for(item in projectStatus){
                                            <option value="${item.num}">${item.name}</option>
                                            @}
                                        </select>
                                    </div>

                                    <div class="inline-search-s">
                                        <label class="label-search-s">进场时间</label>
                                        <div class="" id="datepicker" style="display:inline-block;">
                                            <input class="input-search-s" id="queryStartDate" name="establishDate" type="date" onclick="laydate({istime: false, format: 'YYYY-MM-DD'})">
                                            <span class="">-</span>
                                            <input class="input-search-s" id="queryEndDate" name="establishDate" type="date" onclick="laydate({istime: false, format: 'YYYY-MM-DD'})">
                                        </div>
                                    </div>

                                    <div class="inline-search-s">
                                        <label class="label-search-s">进场时间</label>
                                        <div class="" id="datepicker1" style="display:inline-block;">
                                            <input class="input-search-s" id="queryStartDate1" name="establishDate" type="date" onclick="laydate({istime: false, format: 'YYYY-MM-DD'})">
                                            <span class="">-</span>
                                            <input class="input-search-s" id="queryEndDate1" name="establishDate" type="date" onclick="laydate({istime: false, format: 'YYYY-MM-DD'})">
                                        </div>
                                    </div>

                                </div>

                                <div class="col-lg-2 col-sm-2-right">
                                    <#button name="搜索" icon="fa-search" clickFun="ProjectSubContractor.search()"/>
                                </div>

                            </div>
                            <div class="hidden-xs" id="ProjectSubContractorTableToolbar" role="group">
                                <#button name="添加" icon="fa-plus" clickFun="ProjectSubContractor.openAddProjectSubContractor()"/>
                                <!--<#button name="修改" icon="fa-edit" clickFun="ProjectSubContractor.openProjectSubContractorDetail()" space="true"/>-->
                                <#button name="删除" icon="fa-remove" clickFun="ProjectSubContractor.delete()" space="true"/>
                                <#button name="启用" icon="fa-check" clickFun="ProjectSubContractor.changeState(1)" space="true"/>
                                <#button name="禁用" icon="fa-minus-circle" clickFun="ProjectSubContractor.changeState(2)" space="true"/>
                                <#button name="评价" icon="fa-edit" clickFun="ProjectSubContractor.openProjectContractorEvaluation()" space="true"/>
                            </div>
                            <#table id="ProjectSubContractorTable"/>
                        </div>
                    </div>
                </div>

            </div>
            <div id="classList" class="tab-pane">
                <div class="panel-body">
                    <div class="col-sm-12">
                        <div class="row">
                            <div class="col-sm-5 no-before">
                                <div class="ap-container">
                                    <div class="ap-title">
                                        <span id="projectName">${item.projectName}</span>
                                    </div>
                                    <div class="ap-content">
                                        <#table id="TeamMasterTable"/>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-7 no-before">
                                <div class="ap-container">
                                    <div class="ap-title">
                                        <span id="teamName"></span>
                                        <input id="teamSysNo" hidden />
                                    </div>
                                    <div class="ap-content">

                                        <#table id="TeamMemberTable"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>

<style>
    #span_detele{
        display: none;
    }
    .ap-container .ap-title{
        height: 40px;
        line-height: 40px;
        background-color: #f9f9f9;
        border:#EEE solid 1px;
        padding:0 25px;
    }
    .ap-container .ap-content {
        min-height: 400px;
        border:#EEE solid 1px;
        border-top:none;
        padding:25px;
    }
    .ap-container .ap-add {
        width:40px;
        text-align:right;
        float:right;
        font-size: 24px;
        display:inline-block;
        cursor: pointer;
    }
    .span-but {
        padding:0 5px;
        cursor: pointer;
    }
</style>
<script type="text/javascript" src="http://api.map.baidu.com/api?v=3.0&ak=G8Gd8tZvStagwsAL6HX01acf93BenlEk"></script>
<script src="${ctxPath}/static/modular/team/teamMaster/teamMaster.js"></script>
<script src="${ctxPath}/static/modular/project/projectMaster/projectMaster_info.js"></script>
<script src="${ctxPath}/static/modular/project/projectMaster/projectSubContractor.js"></script>
@}
