<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xywg.admin.modular.system.dao.SysPathMapper">

<select id="selectAllSysPaths" resultType="com.xywg.admin.modular.system.model.SysPath">
    SELECT
    id,`key`,`name`,path
    FROM
    sys_path
    WHERE
    status = 1
</select>
</mapper>
